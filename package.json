{
  "name": "{{PACKAGE_NAME}}",
  "version": "1.0.0",
  "description": "{{PACKAGE_DESCRIPTION}}",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "type": "module",
  "scripts": {
    "build": "bun run build:clean && bun run build:compile",
    "build:clean": "rm -rf dist",
    "build:compile": "tsc",
    "dev": "bun run --watch src/index.ts",
    "test": "bun test",
    "test:filter": "bun test --filter",
    "test:compatibility": "bun test automated-cases.test.ts",
    "prepublishOnly": "bun run build && bun test",
    "prepack": "bun run build",
    "init": "bun run scripts/init.ts"
  },
  "keywords": [
    "{{KEYWORDS}}",
    "typescript",
    "go-port"
  ],
  "author": {
    "name": "SubtleTools",
    "url": "https://github.com/SubtleTools"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "{{REPOSITORY_URL}}"
  },
  "bugs": {
    "url": "{{REPOSITORY_URL}}/issues"
  },
  "homepage": "{{REPOSITORY_URL}}#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./go-style": {
      "import": "./dist/go-style.js",
      "types": "./dist/go-style.d.ts"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^24.3.0",
    "bun-types": "^1.2.20",
    "typescript": "latest"
  }
}
